{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "Film weekly service",
    "version" : "0.1.0"
  },
  "servers" : [ {
    "url" : "http://localhost:10010/api/v1"
  } ],
  "paths" : {
    "/events" : {
      "get" : {
        "description" : "Returns list of musical city musicevents",
        "operationId" : "events",
        "parameters" : [ {
          "name" : "date",
          "in" : "query",
          "description" : "FIltering date",
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Success",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/EventsResponse"
                }
              }
            }
          },
          "default" : {
            "description" : "Error",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      },
      "x-swagger-router-controller" : "events-controller"
    },
    "/authors" : {
      "get" : {
        "description" : "Returns list of authors",
        "operationId" : "authors",
        "parameters" : [ {
          "name" : "date",
          "in" : "query",
          "description" : "FIltering date",
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Success",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/AuthorsResponse"
                }
              }
            }
          },
          "default" : {
            "description" : "Error",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      },
      "x-swagger-router-controller" : "authors-controller"
    },
    "/swagger" : {
      "x-swagger-pipe" : "swagger_raw"
    }
  },
  "components" : {
    "schemas" : {
      "EventsResponse" : {
        "type" : "array",
        "items" : {
          "$ref" : "#/components/schemas/Event"
        }
      },
      "Event" : {
        "required" : [ "date" ],
        "type" : "object",
        "properties" : {
          "name" : {
            "type" : "string"
          },
          "street" : {
            "type" : "string"
          },
          "price" : {
            "type" : "string"
          },
          "date" : {
            "type" : "string"
          },
          "imageId" : {
            "type" : "string"
          }
        }
      },
      "AuthorsResponse" : {
        "type" : "array",
        "items" : {
          "$ref" : "#/components/schemas/Author"
        }
      },
      "Author" : {
        "required" : [ "bio", "country", "famous", "name", "portrait" ],
        "type" : "object",
        "properties" : {
          "name" : {
            "type" : "string"
          },
          "bio" : {
            "type" : "string"
          },
          "portrait" : {
            "type" : "string"
          },
          "country" : {
            "type" : "string"
          },
          "famous" : {
            "type" : "string"
          }
        }
      },
      "ErrorResponse" : {
        "required" : [ "message" ],
        "type" : "object",
        "properties" : {
          "message" : {
            "type" : "string"
          }
        }
      }
    }
  }
}